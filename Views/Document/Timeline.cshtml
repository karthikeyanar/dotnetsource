@{
	var _PageHeaderModel = new dotnetsource.Models.PageHeaderModel { Title = "Timeline" };
	var _BreadCrumbModel = new dotnetsource.Models.BreadCrumbModel {
		FirstUrl = "/Home/Index", FirstUrlName = "Home",
		SecondUrl = "/Home/SignIn", SecondUrlName = "Pages",
		ThirdUrl = "/Home/Timeline", ThirdUrlName = "Timeline"
	};
	ViewBag.Title = _PageHeaderModel.Title;
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@section styles {
	<!-- timeline -->
	<link href="/css/pages/timeline.css" rel="stylesheet" type="text/css" />
}
@Html.Partial("_PageHeader", _PageHeaderModel)
@Html.Partial("_BreadCrumb", _BreadCrumbModel)
<div class="row">
	<div class="col-md-12">
		<h4>
			Time Line</h4>
		<div class="notes notes-primary">
			<p><strong>@dotnetsource.Helpers.Common.SiteName</strong> introduces timeline css class</p>
			<p>
				Sign In page contains use button component so you may want to check out <a href="/Document/Buttons" target="_blank">Buttons</a> pages.</p>
		</div>
		<pre><code>&lt;ul class="timeline"&gt;
	&lt;li class="time-label"&gt;&lt;span class="bg-danger"&gt;10 Feb. 2014 &lt;/span&gt;&lt;/li&gt;
	&lt;li&gt;&lt;span class="time"&gt;12:05 PM&lt;/span&gt;
		&lt;button type="button" class="btn btn-danger btn-circle btn-lg"&gt;
			&lt;i class="fa fa-envelope"&gt;&lt;/i&gt;
		&lt;/button&gt;
		&lt;div class="timeline-item bg-danger"&gt;
			&lt;h3 class="timeline-header"&gt;
				&lt;a class="title" href="#"&gt;Support Team sent you and email&lt;/a&gt;
			&lt;/h3&gt;
			&lt;div class="timeline-body"&gt;
				&lt;p&gt;
					Etsy doostang zoodles disqus groupon greplin oooj voxy zoodles, weebly ning heekya handango imeem plugg dopplr jibjab, movity jajah plickers sifteo edmodo ifttt zimbra. Babblely odeo kaboodle quora plaxo ideeli hulu weebly balihoo...&lt;/p&gt;
			&lt;/div&gt;
			&lt;div class='timeline-footer'&gt;
				&lt;a href="#" class="pull-right"&gt;Read more&lt;/a&gt;
			&lt;/div&gt;
		&lt;/div&gt;
	&lt;/li&gt;
	&lt;li&gt;&lt;span class="time"&gt;12:10 PM&lt;/span&gt;
		&lt;button type="button" class="btn btn-primary btn-circle btn-lg"&gt;
			&lt;i class="fa fa-user"&gt;&lt;/i&gt;
		&lt;/button&gt;
		&lt;div class="timeline-item bg-primary"&gt;
			&lt;h3 class="timeline-header"&gt;
				&lt;a class="title" href="#"&gt;Sarah Young accepted your friend request&lt;/a&gt;
			&lt;/h3&gt;
		&lt;/div&gt;
	&lt;/li&gt;
	&lt;li&gt;&lt;span class="time"&gt;2:00 PM&lt;/span&gt;
		&lt;button type="button" class="btn btn-success btn-circle btn-lg"&gt;
			&lt;i class="fa fa-comments"&gt;&lt;/i&gt;
		&lt;/button&gt;
		&lt;div class="timeline-item bg-success"&gt;
			&lt;h3 class="timeline-header"&gt;
				&lt;a class="title" href="#"&gt;Jay White commented on your post&lt;/a&gt;
			&lt;/h3&gt;
			&lt;div class="timeline-body"&gt;
				&lt;p&gt;
					Take me to your leader! Switzerland is small and neutral! We are more like Germany, ambitious and misunderstood!&lt;/p&gt;
			&lt;/div&gt;
			&lt;div class='timeline-footer'&gt;
				&lt;a href="#" class="pull-right"&gt;View comment&lt;/a&gt;
			&lt;/div&gt;
		&lt;/div&gt;
	&lt;/li&gt;
	&lt;li class="time-label"&gt;&lt;span class="bg-success"&gt;3 Jan. 2014 &lt;/span&gt;&lt;/li&gt;
	&lt;li&gt;&lt;span class="time"&gt;10:00 AM&lt;/span&gt;
		&lt;button type="button" class="btn btn-info btn-circle btn-lg"&gt;
			&lt;i class="fa fa-camera"&gt;&lt;/i&gt;
		&lt;/button&gt;
		&lt;div class="timeline-item bg-purple"&gt;
			&lt;h3 class="timeline-header"&gt;
				&lt;a class="title" href="#"&gt;Mina Lee uploaded new photos&lt;/a&gt;
			&lt;/h3&gt;
			&lt;div class="timeline-body"&gt;
				&lt;img src="/img/1.jpg" /&gt;
				&lt;img src="/img/2.jpg" /&gt;
				&lt;img src="/img/3.jpg" /&gt;
				&lt;img src="/img/4.jpg" /&gt;
			&lt;/div&gt;
		&lt;/div&gt;
	&lt;/li&gt;
	&lt;li&gt;&lt;span class="time"&gt;11:00 AM&lt;/span&gt;
		&lt;button type="button" class="btn btn-warning btn-circle btn-lg"&gt;
			&lt;i class="fa fa-video-camera"&gt;&lt;/i&gt;
		&lt;/button&gt;
		&lt;div class="timeline-item bg-warning"&gt;
			&lt;h3 class="timeline-header"&gt;
				&lt;a class="title" href="#"&gt;Mr. Doe shared a video&lt;/a&gt;
			&lt;/h3&gt;
			&lt;div class="timeline-body"&gt;
				&lt;iframe width="300" height="169" src="http://www.youtube.com/embed/fLe_qO4AE-M" frameborder="0" allowfullscreen&gt;&lt;/iframe&gt;
			&lt;/div&gt;
			&lt;div class="timeline-footer"&gt;
				&lt;a href="#" class="pull-right"&gt;See comments&lt;/a&gt;
			&lt;/div&gt;
		&lt;/div&gt;
	&lt;/li&gt;
	&lt;li&gt;
		&lt;button type="button" class="btn btn-warning btn-circle btn-lg"&gt;
			&lt;i class="fa fa-clock-o"&gt;&lt;/i&gt;
		&lt;/button&gt;
	&lt;/li&gt;
&lt;/ul&gt;
</code></pre>
	</div>
</div>
