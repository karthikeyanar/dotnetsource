@{
	var _PageHeaderModel = new dotnetsource.Models.PageHeaderModel { Title = "Wells" };
	var _BreadCrumbModel = new dotnetsource.Models.BreadCrumbModel {
		FirstUrl = "/Home/Index", FirstUrlName = "Home",
		SecondUrl = "/Home/General", SecondUrlName = "UI Features",
		ThirdUrl = "/Home/Wells", ThirdUrlName = "Wells"
	};
	ViewBag.Title = _PageHeaderModel.Title;
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("_PageHeader", _PageHeaderModel)
@Html.Partial("_BreadCrumb", _BreadCrumbModel)
<div class="row">
	<div class="col-md-12">
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title">
					Background Colors</h3>@Html.Partial("_PanelCloseTools")
			</div>
			<div class="panel-body">
				<div class="notes notes-primary">
					<p>@dotnetsource.Helpers.Common.SiteName CSS provides access to the main colors. You can sets: background, foreground for any elements.
					</p>
					<p class="description">
						To set background color: use prefix <code>bg</code>, to set foreground color: use prefix <code>fg</code>. Also you can set colors with classes <code>bg-*</code>, <code>fg-*</code>.
					</p>
				</div>
				<div class="row">
					<div class="col-md-2">
						<pre><code>.bg-primary</code></pre>
						<div class="well bg-primary text-center">
							Primary</div>
					</div>
					<div class="col-md-2">
						<pre><code>.bg-success</code></pre>
						<div class="well bg-success text-center">
							Success</div>
					</div>
					<div class="col-md-2">
						<pre><code>.bg-info</code></pre>
						<div class="well bg-info text-center">
							Info</div>
					</div>
					<div class="col-md-2">
						<pre><code>.bg-warning</code></pre>
