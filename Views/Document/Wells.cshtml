@{
    var _PageHeaderModel = new dotnetsource.Models.PageHeaderModel { Title = "Wells" };
    var _BreadCrumbModel = new dotnetsource.Models.BreadCrumbModel
    {
        FirstUrl = "/Home/Index",
        FirstUrlName = "Home",
        SecondUrl = "/Home/General",
        SecondUrlName = "UI Features",
        ThirdUrl = "/Home/Wells",
        ThirdUrlName = "Wells"
    };
    ViewBag.Title = _PageHeaderModel.Title;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("_PageHeader", _PageHeaderModel)
@Html.Partial("_BreadCrumb", _BreadCrumbModel)
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-pink">
            <div class="panel-heading">
                <h3 class="panel-title">Wells</h3>@Html.Partial("_PanelCloseTools")
            </div>
            <div class="panel-body">
                <p>
                    Wells are based on <a href="http://localhost:1010/Document/Wells" target="_blank">bootstrap wells</a>
                </p>
            </div>
        </div>
        <div class="panel panel-info">
            <div class="panel-heading">
                <h3 class="panel-title">Wells - Colors</h3>@Html.Partial("_PanelCloseTools")
            </div>
            <div class="panel-body">
                <p>
                    Wells with different colors<code>.bg-primary,.bg-success etc...</code>
                </p>
                <pre><code>&lt;div class="well bg-primary no-border"&gt;
    &lt;h4&gt;
        Primary
    &lt;/h4&gt;
    &lt;p&gt;
        Content...
    &lt;/p&gt;
&lt;/div&gt;</code>
</pre><pre><code>&lt;div class="well bg-success no-border"&gt;
    &lt;h4&gt;
        Primary
    &lt;/h4&gt;
    &lt;p&gt;
        Content...
    &lt;/p&gt;
&lt;/div&gt;</code>
</pre>
            </div>
        </div>
    </div>
</div>
