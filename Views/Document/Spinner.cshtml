@{
	var _PageHeaderModel = new dotnetsource.Models.PageHeaderModel { Title = "Spinner" };
	var _BreadCrumbModel = new dotnetsource.Models.BreadCrumbModel {
		FirstUrl = "/Home/Index", FirstUrlName = "Home",
		SecondUrl = "/Home/Modal", SecondUrlName = "Plugins",
		ThirdUrl = "/Home/Spinner", ThirdUrlName = "Spinner"
	};
	ViewBag.Title = _PageHeaderModel.Title;
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("_PageHeader", _PageHeaderModel)
@Html.Partial("_BreadCrumb", _BreadCrumbModel)
@section styles {
}
@section scripts {
}
<div class="row">
	<div class="col-md-12">
		<div class="panel panel-primary">
			<div class="panel-heading">
				<h3 class="panel-title">
					Spinner</h3>@Html.Partial("_PanelCloseTools")
			</div>
			<div class="panel-body">
				<h4>
					Popover Confirm</h4>
				<div class="notes notes-primary">
					<p>
						A mobile and touch friendly input spinner component for Bootstrap 3. It supports the mousewheel and the up/down keys. Read more about it on <a href="http://www.virtuosoft.eu/code/bootstrap-touchspin/">Spinner plugin website</a>.
					</p>
				</div>
				<h4>
					Js Files</h4>
				<pre><code>js/plugins/bootstrap-touchspin/bootstrap.touchspin.js</code></pre>
			</div>
		</div>
	</div>
</div>
