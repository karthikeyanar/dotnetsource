@{
	var _PageHeaderModel = new dotnetsource.Models.PageHeaderModel { Title = "Icons" };
	var _BreadCrumbModel = new dotnetsource.Models.BreadCrumbModel {
		FirstUrl = "/Home/Index", FirstUrlName = "Home",
		SecondUrl = "/Home/General", SecondUrlName = "UI Features",
		ThirdUrl = "/Home/Icons", ThirdUrlName = "Icons"
	};
	ViewBag.Title = _PageHeaderModel.Title;
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("_PageHeader", _PageHeaderModel)
@Html.Partial("_BreadCrumb", _BreadCrumbModel)
@section styles {
	<link href="/css/pages/fonts.css" rel="stylesheet" type="text/css" />
}
<div class="tab-fancy">
    <!-- Nav tabs -->
    <ul class="nav nav-tabs">
        <li class="active"><a href="#icon1" data-toggle="tab"><span>Font Awesome</span></a></li>
        <li><a href="#icon2" data-toggle="tab"><span>Glyphicons</span></a></li>
    </ul>
    <!-- Tab panes -->
    <div class="tab-content padding-top-10">
        <div class="tab-pane active" id="icon1">
            <div class="row">
                <div class="col-md-12">
                    <h4>Font Awesome Icons</h4>
                    <div class="notes notes-primary">
                        <p>
                            Font Awesome is iconic font designed for
                            use with Twitter Bootstrap. It contains 220 icons in pictographic font form.
                        </p>
                        <p>Read more at <a href="http://fortawesome.github.com/Font-Awesome/" target="_blank">Font Awesome website</a>.</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="tab-pane" id="icon2">
            <div class="row">
                <div class="col-md-12">
                    <h4>Glyphicons Icons</h4>
                    <div class="notes notes-primary">
                        <p>
                            140 icons in sprite form, available in gray (altered for Light Blue) and white,
                            provided by <a href="http://getbootstrap.com/components/#glyphicons" target="_blank">Glyphicons</a>.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>