@{
	var _PageHeaderModel = new dotnetsource.Models.PageHeaderModel { Title = "Wells" };
	var _BreadCrumbModel = new dotnetsource.Models.BreadCrumbModel {
		FirstUrl = "/", FirstUrlName = "Home",
		SecondUrl = "/Home/General", SecondUrlName = "UI Features",
		ThirdUrl = "/Home/Wells", ThirdUrlName = "Wells"
	};
	ViewBag.Title = _PageHeaderModel.Title;
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("_PageHeader", _PageHeaderModel)
@Html.Partial("_BreadCrumb", _BreadCrumbModel)
<div class="row">
	<div class="col-md-12">
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title">
					Background Colors</h3>@Html.Partial("_PanelCloseTools")
			</div>
			<div class="panel-body">
				<div class="row">
					<div class="col-md-2">
						<div class="well bg-primary text-center">
							Primary</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-success text-center">
							Success</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-info text-center">
							Info</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-warning text-center">
							Warning</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-danger text-center">
							Danger</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-purple text-center">
							Purple</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-cardovan text-center">
							Cardovan</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-teal text-center">
							Teal</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-pink text-center">
							Pink</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-dark-blue text-center">
							Dark Blue</div>
					</div>
					<div class="col-md-2">
						<div class="well bg-dark-blue text-center">
							Blue</div>
					</div>
				</div>
			</div>
		</div>
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title">
					Foreground Colors</h3>@Html.Partial("_PanelCloseTools")
			</div>
			<div class="panel-body">
				<div class="row">
					<div class="col-md-2">
						<div class="well bg-black fg-white text-center">
							White</div>
					</div>
					<div class="col-md-2">
						<div class="well fg-black text-center">
							Black</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
