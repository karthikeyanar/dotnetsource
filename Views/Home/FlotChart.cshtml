@{
	var _PageHeaderModel = new dotnetsource.Models.PageHeaderModel { Title = "Flot Chart" };
	var _BreadCrumbModel = new dotnetsource.Models.BreadCrumbModel {
		FirstUrl = "/", FirstUrlName = "Home",
		SecondUrl = "/Home/Modal", SecondUrlName = "Plugins",
		ThirdUrl = "/Home/FlotChart", ThirdUrlName = "Flot Chart"
	};
	ViewBag.Title = _PageHeaderModel.Title;
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("_PageHeader", _PageHeaderModel)
@Html.Partial("_BreadCrumb", _BreadCrumbModel)
@section scripts {
	<!-- flot chart -->
	<script src="/js/plugins/flot/jquery.flot.min.js" type="text/javascript"></script>
	<script src="/js/plugins/flot/jquery.flot.pie.min.js" type="text/javascript"></script>
	<script src="/js/plugins/flot/jquery.flot.navigate.min.js" type="text/javascript"></script>
	<script src="/js/plugins/flot/jquery.flot.resize.min.js" type="text/javascript"></script>
	<script src="/js/pages/flot-chart.js" type="text/javascript"></script>
}
<div class="row">
	<div class="col-lg-6">
		<div class="panel panel-primary">
			<div class="panel-heading">
				<h3 class="panel-title">
					Pie Flot Chart</h3>@Html.Partial("_PanelCloseTools")
			</div>
			<div class="panel-body">
				<div id="pie-chart1" class="chart chart-medium">
				</div>
			</div>
		</div>
	</div>
	<div class="col-lg-6">
		<div class="panel panel-cardovan">
			<div class="panel-heading">
				<h3 class="panel-title">
					Interactivity</h3>
			</div>
			<div class="panel-body">
				<div id="interactivity" class="chart chart-medium">
				</div>
			</div>
		</div>
	</div>
</div>
<div class="row">
	<div class="col-lg-12">
		<div class="panel panel-success">
			<div class="panel-heading">
				<h3 class="panel-title">
					Panning and Zooming</h3>@Html.Partial("_PanelCloseTools")
			</div>
			<div class="panel-body">
				<div id="panning" class="chart chart-large">
				</div>
			</div>
		</div>
	</div>
</div>
