		FirstUrl = "/", FirstUrlName = "Home",
		SecondUrl = "/Home/BoxLayout", SecondUrlName = "Layout",
		ThirdUrl = "/Home/SidebarFixed", ThirdUrlName = "Sidebar Fixed"
	};
	ViewBag.Title = _PageHeaderModel.Title;
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@section styles {
	@Html.Partial("_IndexStyles")
}
@section scripts {
	@Html.Partial("_IndexScripts")
}
@Html.Partial("_PageHeader", _PageHeaderModel)
@Html.Partial("_BreadCrumb", _BreadCrumbModel)
@Html.Partial("_Index")
